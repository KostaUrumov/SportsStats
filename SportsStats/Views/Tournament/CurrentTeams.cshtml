@model TournamentViewModel

<div class="text-center">
    <h3>@Model.Name Teams</h3>
</div>

<table class="table table-striped">
    <thead>
        <tr>
            
            <th scope="col">Name</th>
        </tr>
    </thead>

    @for (int i = 0; i < Model.Teams.Count; i++)
    {
        int number = @i + 1;
        <tbody>
            <tr>
                <td>@Model.Teams[i].Name</td>
            </tr>
        </tbody>
    }
</table>
